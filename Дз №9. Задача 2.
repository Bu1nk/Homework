len_max=0
div_max =[]
for num in range (1234,4321+1):
    divs =set()
    for d in range(1,round(num**0.5)+1):
        if num % d == 0:
            divs.add(d)
            divs.add(int(num/d))
        if len(divs) >= len_max:
            len_max =len(divs)
            div_max =divs
print(len_max,sum(div_max))
